# Supabase (create project at https://supabase.com)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# JWT for wallet auth (generate with: openssl rand -base64 32)
JWT_SECRET=your-jwt-secret-at-least-32-chars

# Server
PORT=4000
NODE_ENV=development

# Optional: CORS origin for frontend (defaults to * in dev)
CORS_ORIGIN=http://localhost:5173

# AI pitching engine (Claude)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Netlify (autonomous build pipeline deploy)
NETLIFY_ACCESS_TOKEN=your-netlify-personal-access-token
NETLIFY_TEAM_ID=optional-team-slug-for-site-creation
